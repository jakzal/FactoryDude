{
    "require": {
        "php": ">=5.3.3",
        "pimple/pimple": "*"
    },
    "require-dev": {
        "phpspec/phpspec2": "*@dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jakzal/phpspec2"
        }
    ],
    "autoload": {
        "psr-0": {
            "FactoryDude": "src/",
            "spec": "."
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
